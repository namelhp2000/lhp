    <script type="text/javascript" src="~/RoadFlowResources/scripts/jqgrid/js/jquery.jqGrid.js"></script>

    <script type="text/javascript" src="~/RoadFlowResources/scripts/My97DatePicker/WdatePicker.js"></script>
    <script type="text/javascript" src="~/RoadFlowResources/scripts/jquery.nicescroll.min.js"></script>
    <script type="text/javascript" src="~/RoadFlowResources/scripts/roadui/roadui.init.js?version=@RoadFlow.Utility.Config.SystemVersion"></script>

</head>

<body>

    @RenderBody()
    @RenderSection("scripts", required: false)
</body>
</html>